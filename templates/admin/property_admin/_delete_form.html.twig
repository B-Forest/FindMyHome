<form method="post" action="{{ path('app_property_admin_delete', {'id': property.id}) }}" onsubmit="return confirm('Voulez vous supprimer ce type de transaction ?');" class="focus:outline-none text-white bg-red-700 hover:bg-red-800 focus:ring-4 focus:ring-red-300 font-medium rounded-lg text-sm px-5 py-2.5 mr-2 mb-2 dark:bg-red-600 dark:hover:bg-red-700 dark:focus:ring-red-900">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ property.id) }}">
    <button class="btn">Supprimer</button>
</form>
