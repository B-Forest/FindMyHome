{% extends 'admin/base.html.twig' %}

{% block title %}Visit{% endblock %}

{% block body %}
    <h1>Visit</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ visit.id }}</td>
            </tr>
            <tr>
                <th>DateStart</th>
                <td>{{ visit.dateStart ? visit.dateStart|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>DateEnd</th>
                <td>{{ visit.dateEnd ? visit.dateEnd|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_visit_admin_index') }}">back to list</a>

    <a href="{{ path('app_visit_admin_edit', {'id': visit.id}) }}">edit</a>

    {{ include('admin/visit_admin/_delete_form.html.twig') }}
{% endblock %}
